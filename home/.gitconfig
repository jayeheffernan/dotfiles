[push]
	default = simple
[alias]
	co = checkout
	tco = checkout
	b = branch
	tb = branch
	s = status
	ts = status
	tl = log --oneline
	a = add
	ta = add
	d = diff -w
	td = diff -w
	c = commit
	ca = commit --amend
	cane = commit --amend --no-edit
